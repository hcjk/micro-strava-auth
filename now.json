{
  "version": 2,
  "name": "micro-strava-auth",
  "builds": [
    {
      "src": "api/login.js",
      "use": "@now/node"
    },
    {
      "src": "api/callback.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "api/login.js"
    },
    {
      "src": "/callback",
      "dest": "api/callback.js"
    }
  ]
}
